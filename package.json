{
  "name": "bedbot",
  "version": "0.0.3",
  "description": "A Discord bot for managing and integrating with a Minecraft Server",
  "repository": "https://github.com/Jkdrumm/MinecraftDiscord",
  "main": "index.js",
  "scripts": {
    "start": "ts-node index.ts",
    "start:server": "ts-node src/modules/server/server.ts",
    "build": "tsc && pkg -t node16-win javascript/src/modules/server/server.js --out-path build && pkg -t node16-win javascript/index.js --output build/discordBot",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:debug": "jest --detectOpenHandles",
    "test:coverage": "jest --coverage",
    "test:coverage:watch": "jest --coverage --watch"
  },
  "author": "Joshua Drumm",
  "license": "UNLICENSED",
  "dependencies": {
    "@discordjs/collection": "^0.4.0",
    "adm-zip": "^0.5.9",
    "axios": "^0.24.0",
    "discord.js": "^13.6.0",
    "minecraft-server-util": "3.4.4",
    "named-pipes": "0.0.1",
    "properties-reader": "^2.2.0",
    "ps-node": "^0.1.6",
    "signal-exit": "^3.0.3",
    "superagent": "^6.1.0"
  },
  "devDependencies": {
    "@types/adm-zip": "^0.4.34",
    "@types/jest": "^27.4.0",
    "@types/node": "^17.0.10",
    "@types/properties-reader": "^2.1.1",
    "@types/ps-node": "^0.1.1",
    "@types/signal-exit": "^3.0.1",
    "@types/superagent": "^4.1.14",
    "jest": "^27.4.7",
    "mock-stdin": "^1.0.0",
    "pkg": "^5.5.2",
    "ts-jest": "^27.1.2",
    "ts-node": "^10.4.0",
    "typescript": "^4.5.4"
  },
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node",
    "moduleFileExtensions": [
      "js",
      "ts"
    ],
    "setupFilesAfterEnv": [
      "./tests/setup.ts"
    ],
    "testMatch": [
      "**/*.(test.ts)"
    ],
    "collectCoverageFrom": [
      "./**.ts"
    ],
    "coverageReporters": [
      "lcov",
      "text",
      "text-summary"
    ]
  }
}
